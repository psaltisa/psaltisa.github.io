  <span class="na">author</span> <span class="p">=</span> <span class="s">{Mertzimekis, T.J and Stamou, K and Psaltis, A}</span><span class="p">,</span>
  <span class="na">journal</span> <span class="p">=</span> <span class="s">{Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment}</span><span class="p">,</span>
  <span class="na">volume</span> <span class="p">=</span> <span class="s">{807}</span><span class="p">,</span>
